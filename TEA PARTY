#include<stdio.h>
int main()
{
int tea,candy;
printf(" enter the no of tea:");
scanf("%d",&tea);
printf("enter the no of candy:");
scanf("%d",&candy);
if(tea<5||candy<5)
{
    printf("0");
}
else if(tea>=2*candy||candy>=2*tea)
{
    printf("2");
}
else if(tea>=5&&candy>=5)
{
    printf("1");
}
return 0;
}
